{
  "name": "telegram-bingo-bot",
  "version": "1.0.0",
  "description": "Telegram Bingo Bot with Vue.js Mini App",
  "main": "bot.js",
  "type": "commonjs",
  "scripts": {
    "start": "node bot.js",
    "dev": "concurrently \"npm run dev:bot\" \"npm run dev:web\"",
    "dev:bot": "nodemon bot.js",
    "dev:web": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build",
    "setup-webhook": "node setup-webhook.js",
    "test-webhook": "node test-webhook.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "bingo",
    "vue",
    "mini-app"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.64.0",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
